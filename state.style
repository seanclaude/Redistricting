<Document>
<Style id="state_style">
    <BalloonStyle>
        <text>
            <![CDATA[
            <h2>$[STATE_schema/PARcode]/$[STATE_schema/STATEcode]</h2>
            ]]>
        </text>
    </BalloonStyle>
    <LineStyle>
        <color>ffffffff</color>
        <width>2</width>
    </LineStyle>
    <PolyStyle>
        <fill>0</fill>
    </PolyStyle>
</Style>
<StyleMap id="state_stylemap">
    <Pair>
        <key>normal</key>
        <styleUrl>#state_style</styleUrl>
    </Pair>
    <Pair>
        <key>highlight</key>
        <styleUrl>#state_style</styleUrl>
    </Pair>
</StyleMap>
</Document>
